      background: #FFD8D0;
      border-radius: 0;
      box-shadow: none;
      overflow: hidden;
      box-sizing: border-box;
    }
    .events-inner {
      max-width: 1100px;
      margin: 0 auto;
      padding: 0 3rem;
    }

    .events h2 {
      margin-bottom: 0.4rem;
      text-align: left;
      letter-spacing: 0.02em;
    }
    .events-subtitle {
      text-align: left;
      margin: 0 0 1.6rem;
      color: #3A3F3A;
      font-weight: 600;
      letter-spacing: 0.05em;
      text-transform: uppercase;
      font-size: 0.95rem;
    }

    .events-week {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(140px, 1fr));
      gap: 1rem;
    }

    .event-day-card {
      background: transparent;
      border: 2px solid rgba(255,255,255,0.85);
      border-radius: 1rem;
      padding: 0.85rem 1rem;
      display: flex;
      flex-direction: column;
      gap: 0.5rem;
      min-height: 200px;
      box-shadow: none;
      color: #2f2f2f;
    }

    .event-day-name {
      font-weight: 700;
      letter-spacing: 0.06em;
      text-transform: uppercase;
      font-size: 0.95rem;
      color: #2f2f2f;
    }

    .event-day-empty {
      color: #555;
      font-size: 0.9rem;
    }

    .event-pill {
      display: inline-block;
      margin-top: 0.35rem;
      padding: 0;
      border-radius: 0;
      background: transparent;
      color: #2f2f2f;
      font-size: 0.9rem;
      line-height: 1.4;
      position: relative;
      padding-left: 0.9rem;
    }

    .event-pill + .event-pill {
      margin-top: 0.25rem;
    }

    .event-pill span {
      display: block;
    }

    .event-pill-title {
      font-weight: 700;
      position: relative;
    }

    .event-pill-time {
      opacity: 0.9;
    }

    .event-pill::before {
      content: "•";
      position: absolute;
      left: 0;
      top: 0;
      color: #2f2f2f;
      font-weight: 700;
      line-height: 1.2;
    }

    /* Donation section with combined title */
    #donate {
      margin-top: 2rem;
      margin-bottom: 3rem;
      display: grid;
      grid-template-columns: minmax(0, 1.15fr) minmax(320px, 0.85fr);
