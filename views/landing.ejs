    }

    .steam-strip--large .steam-card:hover {
      transform: translateY(-4px);
      box-shadow: 0 10px 22px rgba(0,0,0,0.22);
    }

    /* Events section: week planner */
    .events {
      position: relative;
      width: 100vw;
      max-width: none;
      margin-left: calc(-50vw + 50%);
      margin-right: calc(-50vw + 50%);
