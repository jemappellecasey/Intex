/* Full-width hero image with overlay text, no card */
.hero {
  position: relative;
}

.hero-image-wrap {
  position: relative;
  width: 100%;
  overflow: hidden;
}

.hero-image {
  width: 100%;
  height: 75vh;
  min-height: 620px;
  object-fit: cover;
  display: block;
}

/* Darken image slightly for readability */
.hero-image-wrap::after {
  content: "";
  position: absolute;
  inset: 0;
  background: rgba(0, 0, 0, 0.28);
  z-index: 1; /* <<< make sure overlay is BELOW the text */
}


/* HERO OVERLAY — back to original structure but improved */
.hero-overlay {
  position: absolute;
  inset: 0;
  z-index: 2;  /* <<< ensures text is above the dark overlay */
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding-top: 3.5rem;
  align-items: center;
  text-align: center;
  color: #FFFFFF; 
  text-shadow: none;
  pointer-events: none;
  position: absolute;
  inset: 0;
  z-index: 2; 
}


/* Main hero title — scale more across breakpoints */
.hero-lead {
  max-width: 920px;
  font-size: clamp(1.2rem, 3vw + 0.7rem, 2.6rem) !important;
  line-height: 1.28;
