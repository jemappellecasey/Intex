    }

    /* STEAM strip in Programs: big, easy to read */
    .steam-strip {
      margin-top: 1.8rem;
      display: flex;
      gap: 1rem;
      flex-wrap: wrap;
      justify-content: center;
    }

    .steam-strip--large .steam-card {
      position: relative;
      overflow: hidden;
      border-radius: 0.9rem;
      flex: 1 1 160px;
      min-width: 160px;
      max-width: none;
      box-shadow: 0 6px 14px rgba(0,0,0,0.18);
      cursor: pointer;
      transition: transform 0.2s ease, box-shadow 0.2s ease;
    }

    .steam-strip--large .steam-card img {
      width: 100%;
      height: 200px;
      object-fit: cover;
      display: block;
      filter: saturate(1.15);
    }

    .steam-strip--large .steam-card__label {
      position: absolute;
      inset: 0;
      display: flex;
      align-items: center;
      justify-content: center;
      font-family: 'TT Milk Script', cursive;
      font-size: 2.1rem;
      color: #FFFFFF;
      text-align: center;
      padding: 0 0.5rem;
      background: linear-gradient(
        to bottom,
        rgba(0,0,0,0.35),
        rgba(0,0,0,0.75)
